<h1>Listing sp500_companies</h1>

<table>
  <tr>
    <th>Symbol</th>
    <th>Company name</th>
    <th>Sec filings</th>
    <th>Gics sector</th>
    <th>Gics sub industry</th>
    <th>Headquarters city</th>
    <th>Sp500 added date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sp500_companies.each do |sp500_company| %>
  <tr>
    <td><%= sp500_company.symbol %></td>
    <td><%= sp500_company.company_name %></td>
    <td><%= sp500_company.sec_filings %></td>
    <td><%= sp500_company.gics_sector %></td>
    <td><%= sp500_company.gics_sub_industry %></td>
    <td><%= sp500_company.headquarters_city %></td>
    <td><%= sp500_company.sp500_added_date %></td>
    <td><%= link_to 'Show', sp500_company %></td>
    <td><%= link_to 'Edit', edit_sp500_company_path(sp500_company) %></td>
    <td><%= link_to 'Destroy', sp500_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

